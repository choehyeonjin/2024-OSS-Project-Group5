{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
    "hosting": {
      "public": "public",
      "rewrites": [
        {
          "source": "/",
          "destination": "/login.html"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
  